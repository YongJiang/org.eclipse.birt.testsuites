                    </structure>
                    <structure>
                        <property name="position">9</property>
                        <property name="name">currency_fk</property>
                        <property name="dataType">string</property>
                    </structure>
                    <structure>
                        <property name="position">10</property>
                        <property name="name">price_per_piece</property>
                        <property name="dataType">decimal</property>
                    </structure>
                </list-property>
            </structure>
            <property name="dataSource">ProductInvoice</property>
            <property name="queryText">select *, (net_amount / quantity) as price_per_piece from
(
	select pe.pricelist_entity_cipher, 'enota'::text as unit, lp.local_name, COALESCE(SUM(alf.value), 1) as quantity,
		sum(ins.amount) as amount, sum(net_amount) as net_amount,
		sum(tax_amount) as tax_amount, ins.tax_rate, s.currency_fk
	from invoice_specification ins
	join pricelist_entity pe ON pe.id = ins.pricelist_entity_fk
	left join aggregate_log_final alf ON alf.company_fk = ins.company_fk and alf.pricelist_entity_fk = ins.pricelist_entity_fk and alf.billing_job_fk = ins.billing_job_fk
	join local_pricelist lp ON lp.id = ins.local_pricelist_fk
	join company c ON c.id = ins.company_fk
	join franchise f ON f.id = c.franchise_fk
	join state s ON s.code = f.state_fk
	where ins.billing_job_fk = ?
	and (ins.company_fk = ? or c.payer_fk = ?)
	group by pricelist_entity_cipher, lp.local_name, ins.tax_rate, currency_fk
) as sub
where quantity > 0	--exclude invoice specifications with quantity 0 or less
order by pricelist_entity_cipher</property>
