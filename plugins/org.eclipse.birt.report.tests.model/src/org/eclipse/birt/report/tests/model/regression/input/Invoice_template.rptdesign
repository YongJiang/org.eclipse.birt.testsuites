            <property name="dataSource">ProductInvoice</property>
            <property name="queryText">select i.id, amount, (amount / er.rate_eur) as amount_eur, net_amount, amount_eur, tax_amount, issue_date, expiration_date, invoice_number, reference_key,
	c.id as "company_id", c.contract_number as "c_contract_number",
	c.name as "c_name", 
	c.billing_address_street as "c_address_street", 
	c.billing_address_post_code as "c_address_pobox", 
	c.billing_address_city as "c_address_city", 
	c.email as "c_email", c.tax_num as "c_tax_num", c.bank_account as "c_bank_account",
	f.name as "f_name", f.address_street as "f_address_street", f.address_post_code as "f_address_pobox", f.address_city as "f_address_city", 
	f.tax_num as "f_tax_num", f.bank_account as "f_bank_account", f.bank_name as "f_bank_name", f.registration_num as "f_registration_num", 
	f.court_name as "f_court_name", f.court_num as "f_court_num", f.responsible_person as "f_responsible_person", f.email as "f_email", 
	s.currency_fk as "f_currency",
	bj.billing_from as "bj_billing_from", (bj.billing_to - interval '1 day') as "bj_billing_to"
from invoice i
join company c ON c.id = i.payer_fk
join franchise f ON f.id = c.franchise_fk
join state s ON s.code = f.state_fk
join billing_job bj ON bj.id = billing_job_fk
left join exchange_rate er ON er.state_fk = s.code
where billing_job_fk = ?
and i.franchise_fk = ?
and (i.id >= ? and i.id &lt;= ?)
order by invoice_number</property>
