            <property name="columns">1</property>
            <property name="columnSpacing">0.25mm</property>
        </graphic-master-page>
    </page-setup>
    <body>
        <grid name="My grid1">
            <row>
                <cell>
                    <property name="column">1</property>
                </cell>
                <cell>
					<property name="column">2</property>
                </cell>
            </row>        
            <row>
                <cell>
                	<property name="column">1</property>
                </cell>
				<cell>
                	<property name="column">2</property>
            	</cell>                    
            </row>
        </grid>
        <grid name="My grid2">
            <column>
                <property name="repeat">2</property>
                <property name="color">red</property>
            </column>
            <row>
                <cell>
                    <property name="column">1</property>
                </cell>
				<cell>
                    <property name="column">2</property>
                    <property name="rowSpan">2</property>
                </cell>                    
            </row>
            <row>
            	 <cell>
                    <property name="column">1</property>
                </cell>
            </row>
        </grid>               
        <grid name="My grid3">
            <column>
                <property name="repeat">2</property>
                <property name="color">red</property>
            </column>
            <row>
            	<cell>
                	<property name="column">1</property>
                </cell>
				<cell>
                    <property name="column">2</property>
                </cell>                                        
            </row>
            <row>
 				<cell>
                	<property name="column">1</property>
	                <property name="rowSpan">2</property>
    	            <property name="colSpan">2</property>
               	</cell>                                        
            </row>                
        </grid>                
        
        <grid name="CopyGrid1">
            <column>
                <property name="repeat">2</property>
                <property name="color">aqua</property>
            </column>
            <row>
				<cell>
                    <property name="column">1</property>
                </cell>   
				<cell>
                    <property name="column">2</property>
                </cell>                                                           
            </row>       
            <row>
				<cell>
                    <property name="column">1</property>
                </cell>   
				<cell>
                    <property name="column">2</property>
                </cell>                                       
            </row>                        
        </grid>    
 		<grid name="PasteGrid1">
            <column>
                <property name="color">yellow</property>
            </column>
            <column>
                <property name="color">red</property>
            </column>            
            <row>
				<cell>
					<property name="column">1</property>
                    <property name="color">red</property>
                    <property name="rowSpan">2</property>
                </cell>     
				<cell>
                    <property name="column">2</property>
                </cell>                                                         
            </row>        
            <row>
				<cell>
                    <property name="column">2</property>
                </cell>                                        
            </row>                      
        </grid>      
              
        <grid name="CopyTable2">
            <column>
                <property name="color">yellow</property>
            </column>
            <column>
                <property name="color">red</property>
            </column>            
            <property name="dataSet">firstDataSet</property>          
            <row>
				<cell>
					<property name="column">1</property>
                    <property name="color">red</property>
                    <property name="rowSpan">2</property>
                    <label/>
                </cell>     
				<cell>
                    <property name="column">2</property>
                </cell>                                                         
            </row>        
            <row>
				<cell>
                    <property name="column">2</property>
                </cell>                                        
            </row>                          
        </grid>    
 		<grid name="PasteTable2">
			<column>
                <property name="repeat">2</property>
                <property name="color">aqua</property>
            </column>
            <row>
				<cell>
                    <property name="column">1</property>
                </cell>   
				<cell>
                    <property name="column">2</property>
                </cell>                                                           
            </row>       
            <row>
				<cell>
                    <property name="column">1</property>
                </cell>   
				<cell>
                    <property name="column">2</property>
                </cell>                                       
            </row>                        
        </grid>                  
    </body>
</report>
